/* pages/questionaire/questionaire.wxss */

.button {
  position: relative;
  display: block;
  width: 184px;
  margin-left: 15%;
  margin-right: auto;
  padding: 12px 24px;
  box-sizing: border-box;
  font-weight: 500;
  font-size: 17px;
  text-align: center;
  text-decoration: none;
  color: #fff;
  line-height: 1.41176471;
  border-radius: 8px;
  background-color: var(--weui-BRAND);
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  user-select: none;
}